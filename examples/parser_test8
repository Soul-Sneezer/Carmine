def xor10(a[10], b[10]) -> out[10]
{
    out = (a or b) and not(a and b);
}

def xor0(a[10], b[10]) -> out[10]
{
    out[0] = (a[0] or b[0]) and not(a[0] and b[0]);
}

def xor1(a[10], b[10]) -> out[10]
{
    out = (a[0] or b[0]) and not(a[0] and b[0]);
}

